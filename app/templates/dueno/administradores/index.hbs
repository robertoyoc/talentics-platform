{{link-to 'Nuevo' 'dueno.administradores.nuevo'}}

<table style="width:100%">
  <tr>
    <th>Nombre</th>
    <th>Apellido</th>
    <th>Acciones</th>
  </tr>
  {{#each model as |cuenta|}}
  <tr>
    <td>{{cuenta.nombre}}</td>
    <td>{{cuenta.apellido}}</td>
    <th>{{link-to 'Editar' 'dueno.administradores.editar' cuenta.id}}<button {{action "delete" cuenta}}>Borrar</button></th>  
  </tr>
  {{/each}}
</table>