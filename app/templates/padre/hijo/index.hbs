{{link-to 'Nuevo' 'padre.hijo.nuevo'}}


<table style="width:100%" class="responsive-table">
  <tr>
    <th>Nombre</th>
    <th>Apellido</th>
    <th>Nacimiento</th>
    <th>Acciones</th>
  </tr>
  {{#each model as |hijo|}}
  <tr>
    <td>{{hijo.name}}</td>
    <td>{{hijo.lastName}}</td>
    <td> {{link-to 'Editar' 'padre.hijo.editar' hijo.id}} <button {{action "delete" hijo}}> Borrar </button></td> 

    
  </tr>
  {{/each}}
</table>
